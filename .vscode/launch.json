  "version": "0.2.0",
  "configurations": [
    {
      "name": "JSON REST",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "cwd": "${workspaceFolder}/json_rest",
      "env": {
        "FLASK_APP": "server.py",
        "FLASK_ENV": "development"
      },
      "args": ["run", "--no-debugger"],
      "jinja": true
    },
    {
      "name": "JSON RPC",
      "type": "python",
      "request": "launch",
      "module": "flask",
      "cwd": "${workspaceFolder}/json_rpc",
      "env": {
        "FLASK_APP": "server.py",
        "FLASK_ENV": "development"
      },
      "args": ["run", "--no-debugger"],
      "jinja": true
    },
    {
      "name": "PROTOBUF_GRPC",
      "type": "python",
      "request": "launch",
      "cwd": "${workspaceFolder}/protobuf_grpc",
      "program": "server.py"
    }
  ]
}
